# EducateWorkforce (default site)
if "{{ EDUCATEWORKFORCE_LMS_HOST }}" not in ALLOWED_HOSTS:
    ALLOWED_HOSTS.append("{{ EDUCATEWORKFORCE_LMS_HOST }}")

if "{{ EDUCATEWORKFORCE_MFE_HOST }}" not in ALLOWED_HOSTS:
    ALLOWED_HOSTS.append("{{ EDUCATEWORKFORCE_MFE_HOST }}")

if "https://{{ EDUCATEWORKFORCE_LMS_HOST }}" not in CORS_ORIGIN_WHITELIST:
    CORS_ORIGIN_WHITELIST.append("https://{{ EDUCATEWORKFORCE_LMS_HOST }}")

if "https://{{ EDUCATEWORKFORCE_MFE_HOST }}/{{ MFE_LEARNING_MFE_APP["name"] }}" not in CORS_ORIGIN_WHITELIST:
    CORS_ORIGIN_WHITELIST.append("https://{{ EDUCATEWORKFORCE_MFE_HOST }}/{{ MFE_LEARNING_MFE_APP["name"] }}")

