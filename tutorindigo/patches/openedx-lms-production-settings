
# Caregiver
if "{% if ENABLE_HTTPS %}{{ CAREGIVER_LMS_HOST }}{% else %}{{ CAREGIVER_LMS_HOST }}:8000{% endif %}" not in ALLOWED_HOSTS:
    ALLOWED_HOSTS.append("{{ CAREGIVER_LMS_HOST }}")
    ALLOWED_HOSTS.append("{{ CAREGIVER_MFE_HOST }}")
    CORS_ORIGIN_WHITELIST.append("{% if ENABLE_HTTPS %}https://{{ CAREGIVER_LMS_HOST }}{% else %}http://{{ CAREGIVER_LMS_HOST }}:{{ MFE_LEARNING_MFE_APP["port"] }}{% endif %}")
    CORS_ORIGIN_WHITELIST.append("{% if ENABLE_HTTPS %}https://{{ CAREGIVER_MFE_HOST }}{% else %}http://{{ CAREGIVER_MFE_HOST }}:{{ MFE_LEARNING_MFE_APP["port"] }}{% endif %}/{{ MFE_LEARNING_MFE_APP["name"] }}")

