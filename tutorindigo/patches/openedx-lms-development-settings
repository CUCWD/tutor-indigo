
# Caregiver
if "{{ CAREGIVER_LMS_HOST }}" not in ALLOWED_HOSTS:
    ALLOWED_HOSTS.append("{{ CAREGIVER_LMS_HOST }}")
    ALLOWED_HOSTS.append("{{ CAREGIVER_MFE_HOST }}")
    CORS_ORIGIN_WHITELIST.append("http://{{ CAREGIVER_LMS_HOST }}:{{ MFE_LEARNING_MFE_APP["port"] }}")
    # CORS_ORIGIN_WHITELIST.append("http://{{ CAREGIVER_MFE_HOST }}:{{ MFE_LEARNING_MFE_APP["port"] }}/{{ MFE_LEARNING_MFE_APP["name"] }}")

